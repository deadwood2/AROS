include $(SRCDIR)/config/aros.cfg

NOWARN_FLAGS := $(NOWARN_OLD_STYLE_DEFINITION)
USER_CFLAGS := $(NOWARN_FLAGS) -I$(SRCDIR)/$(CURDIR)/../includes
USER_CPPFLAGS := \
        -DHMAC_MD5 \
        -DMINIRES_LIB \
        -D_XOPEN_SOURCE=600 \
        -D__BSD_VISIBLE=1

FILES :=	base64 dst_api dst_support hmac_link md5_dgst prandom

%build_linklib mmake=network-dhcp-libdst\
	libname=dst			\
	files="$(FILES)"
